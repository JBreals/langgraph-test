# OpenRouter API
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Model settings
DEFAULT_MODEL=anthropic/claude-3.5-sonnet
DEFAULT_TEMPERATURE=0.7

# App info (OpenRouter requires these headers)
APP_URL=http://localhost
APP_NAME=LangGraph Agent

# Tools API Keys (optional)
TAVILY_API_KEY=your-tavily-api-key-here
OPENWEATHER_API_KEY=your-openweather-api-key-here

# RAG Vector Store
# - memory: 인메모리 키워드 기반 (기본값, 임베딩 불필요)
# - faiss: FAISS 로컬 벡터 스토어 (임베딩 필요)
# - opensearch: OpenSearch 벡터 스토어 (임베딩 필요)
RAG_STORE_TYPE=memory

# Embeddings (FAISS/OpenSearch 사용 시 필요)
# OpenAI 임베딩 사용 시
OPENAI_API_KEY=your-openai-api-key-here
# 또는 HuggingFace 로컬 임베딩 사용 (pip install langchain-huggingface sentence-transformers)

# OpenSearch (opensearch 사용 시)
OPENSEARCH_URL=http://localhost:9200
OPENSEARCH_INDEX=langgraph-docs

# LangSmith (optional - for tracing)
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_API_KEY=your-langsmith-api-key-here
# LANGCHAIN_PROJECT=langgraph-agent
